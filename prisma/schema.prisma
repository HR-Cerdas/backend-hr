generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type ProfilehrsName {
  first_name String
  last_name  String
}

type ProfileusersName {
  first_name String
  last_name  String
}

model profilehrs {
  id       String         @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  name     ProfilehrsName
  noHp     String
  password String
  username String
}

model profileusers {
  id          String           @id @default(auto()) @map("_id") @db.ObjectId
  age         String
  email       String
  gender      String
  location    String
  name        ProfileusersName
  nationality String
  noHp        String
  password    String
  status      String
  username    String
}
